{
  "name": "ferrugemjs-desktop-example",
  "version": "0.0.6",
  "description": "a simple desktop made with Ferrugemjs",
  "main": false,
  "scripts": {
    "jspm": "./node_modules/.bin/jspm",
    "gulp": "./node_modules/.bin/gulp",
    "watch": "gulp watch",
    "compile": "gulp compile",
    "convert": "gulp template",
    "postinstall": "jspm install",
    "build2dist": "jspm bundle dist/desktop/init-app/init-app.html build.js --skip-source-maps --minify"
  },
  "author": "ferrugemjs",
  "license": "ISC",
  "jspm": {
    "format": "amd",
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.33",
      "es6-shim": "npm:es6-shim@^0.35.1",
      "event-emitter-lite": "npm:event-emitter-lite@^1.0.7",
      "ferrugemjs": "npm:ferrugemjs@^0.11.10",
      "ferrugemjs-router": "npm:ferrugemjs-router@^0.0.11",
      "incremental-dom": "npm:incremental-dom@^0.5.1",
      "promised-xhr": "npm:promised-xhr@^1.2.0",
      "tslib": "npm:tslib@1.6.0"
    },
    "devDependencies": {}
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-ferrugemjs": "^0.11.18",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-typescript": "^3.1.4",
    "gulp-uglify": "^1.5.2",
    "http-proxy-middleware": "^0.14.0",
    "jspm": "^0.16.39",
    "require-dir": "^0.3.0",
    "typescript": "^2.1.5"
  }
}
